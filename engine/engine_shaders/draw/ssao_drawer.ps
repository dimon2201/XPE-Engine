struct VSOut
{
    float4 positionClip : SV_POSITION;
    float2 uv : XPE_UV;
};

struct PSOut
{
    float4 color : SV_Target0;
};

Texture2D<float4> DepthTexture : register(t0);

PSOut ps_main(VSOut psIn)
{
    PSOut psOut;
    psOut.color = DepthTexture.Load(int3(psIn.uv, 0.0));

    return psOut;
}